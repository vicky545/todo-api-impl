<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">
  <servers>
    <server>
      <id>anypoint-exchange-v3</id>
      <username>${env.ANYPOINT_USERNAME}</username>
      <password>${env.ANYPOINT_PASSWORD}</password>
    </server>
    <server>
      <id>mulesoft-private</id>
      <username>${env.ANYPOINT_USERNAME}</username>
      <password>${env.ANYPOINT_PASSWORD}</password>
    </server>
    <server>
      <id>mule-enterprise</id>
      <username>${env.ANYPOINT_USERNAME}</username>
      <password>${env.ANYPOINT_PASSWORD}</password>
    </server>
  </servers>
  <profiles>
    <profile>
      <id>mule-extra-repos</id>
      <activation>
        <activeByDefault>true</activeByDefault>
      </activation>
      <repositories>
        <repository>
          <id>mulesoft-private</id>
          <name>MuleSoft Private Repository</name>
          <url>https://repository.mulesoft.org/nexus/content/repositories/private/</url>
          <layout>default</layout>
        </repository>
      </repositories>
    </profile>
  </profiles>
</settings>
